{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_lab3",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "split1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Date as string,",
				"          Name as string,",
				"          Salary as string,",
				"          Address as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     modifiedBefore: (toTimestamp(1667174400621L))) ~> source1",
				"source1 split(isNull(  toDate(Date, 'dd-mm-yyyy')),",
				"     disjoint: false) ~> split1@(ErrorRows, GoodRows)",
				"split1@GoodRows derive(Filename = 'validatefileschema') ~> derivedColumn1"
			]
		}
	}
}