{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfcloudlabpractise"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDataLakeStorage2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage2'"
		},
		"LS_exceloutputfile_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_exceloutputfile'"
		},
		"LS_excelsource_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_excelsource'"
		},
		"LS_lab3textfile_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_lab3textfile'"
		},
		"LS_powerquery_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_powerquery'"
		},
		"LS_sinktable_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_sinktable'"
		},
		"SqlServer1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer1'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azgen2storagacc.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azgen2storagacc.dfs.core.windows.net/"
		},
		"LS_exceloutputfile_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azgen2storagacc.dfs.core.windows.net/"
		},
		"LS_lab3textfile_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azgen2storagacc.dfs.core.windows.net/"
		},
		"LS_powerquery_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azgen2storagacc.dfs.core.windows.net/"
		},
		"LS_sinktable_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azgen2storagacc.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfcloudlabpractise_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDataLakeStorage2_accountKey": {
						"value": "[parameters('AzureDataLakeStorage2_accountKey')]"
					},
					"LS_exceloutputfile_accountKey": {
						"value": "[parameters('LS_exceloutputfile_accountKey')]"
					},
					"LS_excelsource_connectionString": {
						"value": "[parameters('LS_excelsource_connectionString')]"
					},
					"LS_lab3textfile_accountKey": {
						"value": "[parameters('LS_lab3textfile_accountKey')]"
					},
					"LS_powerquery_accountKey": {
						"value": "[parameters('LS_powerquery_accountKey')]"
					},
					"LS_sinktable_accountKey": {
						"value": "[parameters('LS_sinktable_accountKey')]"
					},
					"SqlServer1_connectionString": {
						"value": "[parameters('SqlServer1_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage2_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage2_properties_typeProperties_url')]"
					},
					"LS_exceloutputfile_properties_typeProperties_url": {
						"value": "[parameters('LS_exceloutputfile_properties_typeProperties_url')]"
					},
					"LS_lab3textfile_properties_typeProperties_url": {
						"value": "[parameters('LS_lab3textfile_properties_typeProperties_url')]"
					},
					"LS_powerquery_properties_typeProperties_url": {
						"value": "[parameters('LS_powerquery_properties_typeProperties_url')]"
					},
					"LS_sinktable_properties_typeProperties_url": {
						"value": "[parameters('LS_sinktable_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adfcloudlabpractise_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adfcloudlabpractise_ArmTemplate_0')]"
			]
		}
	]
}